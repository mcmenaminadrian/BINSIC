10 REM Fibonacci Series
20 LET X = 0
30 LET Y = 1
40 LET COUNT = 0
45 IF COUNT < 1 THEN PRINT "HELLO THERE"
50 DIM Z(40)
60 GOSUB 110
62 FOR I = 0 TO 9
64 PRINT "Fibonacci number ", I, " is ", Z(I)
66 NEXT I
70 END
110 LET Z(COUNT) = X + Y
120 IF (COUNT > 10) THEN GOTO 122 ELSE GOTO 125
122 PRINT "OVER NOW"
123 RETURN
125 PRINT "Now on death march"
130 LET X = Y
140 LET Y = Z(COUNT)
160 LET COUNT = COUNT + 1
170 GOSUB 110
210 RETURN

